   "source": [
    "def fast_exp(base, exp):\n",
    "    x=base\n",
    "    y=1\n",
    "    z=exp\n",
    "    while z != 0:\n",
    "        print(x,y,z)\n",
    "        if z % 2 == 1: y = x*y\n",
    "        z = z//2\n",
    "        x = x**2\n",
    "    print(x,y,z)\n",
    "    return y"
   ]
